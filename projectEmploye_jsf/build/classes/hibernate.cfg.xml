<?xml version="1.0" encoding="UTF-8"?> <!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
	<session-factory name="">
		<property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
		<property name="hibernate.connection.password">prefab</property>
		<property name="hibernate.connection.url">jdbc:mysql://localhost:3306/db_employes</property>
		<property name="hibernate.connection.username">root</property>
		<property name="hibernate.default_schema">db_employes</property>
		<property name="hibernate.id.new_generator_mappings">false</property>
		<property name="hibernate.dialect">org.hibernate.dialect.MySQL5Dialect</property>
		<property name="hibernate.current_session_context_class">org.hibernate.context.internal.ThreadLocalSessionContext</property>
	</session-factory>
</hibernate-configuration>